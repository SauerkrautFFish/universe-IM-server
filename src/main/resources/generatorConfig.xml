<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <classPathEntry location="F:\mysql-connector-java-8.0.11.jar" />

    <context id="DB2Tables" targetRuntime="MyBatis3">
        <!--不生成注释-->
        <commentGenerator>
            <property name="suppressDate" value="true"/>
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>
        <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"
                        connectionURL="jdbc:mysql:///im_bird?useSSL=false"
                        userId="root"
                        password="123456">
        </jdbcConnection>

        <javaTypeResolver >
            <property name="forceBigDecimals" value="false" />
        </javaTypeResolver>

        <!--实体类-->
        <javaModelGenerator targetPackage="edu.yjzxc.universeimserver.entity" targetProject="src\main\java" />

        <!--xxxMapper.xml-->
        <sqlMapGenerator targetPackage="edu.yjzxc.universeimserver.mapper"  targetProject="src\main\resources" />

        <!--mapper接口位置-->
        <javaClientGenerator type="XMLMAPPER" targetPackage="edu.yjzxc.universeimserver.dao"  targetProject="src\main\java" />

        <table tableName="chat_msg" domainObjectName="ChatMsg"  enableCountByExample="true" enableSelectByExample="true" enableUpdateByExample="true" enableDeleteByExample="true" selectByExampleQueryId="true"></table>

    </context>
</generatorConfiguration>